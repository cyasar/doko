   "source": [
    "import numpy as np\n",
    "\n",
    "def F(x):\n",
    "    return np.exp(x) - 1\n",
    "\n",
    "def dF(x):\n",
    "    return np.exp(x)\n",
    "\n",
    "x = 1  # başlangıç tahmini\n",
    "for i in range(5):\n",
    "    J = dF(x)\n",
    "    x = x - J**2 / J * F(x)\n",
    "    print(f\"İterasyon {i+1}, x: {x}\")\n",
    "\n"
   ]
