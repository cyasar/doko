   "source": [
    "import numpy as np\n",
    "\n",
    "def F(x):\n",
    "    return x**2 - 4*x + 4\n",
    "\n",
    "def dF(x):\n",
    "    return 2*x - 4\n",
    "\n",
    "x = 6  # başlangıç tahmini\n",
    "for i in range(1,x):\n",
    "    J = dF(x)\n",
    "    x = x - J**2 / J * F(x)\n",
    "    print(f\"İterasyon {i+1}, x: {x}\")\n",
    "\n"
   ]
